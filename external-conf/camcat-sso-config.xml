<?xml version="1.0" encoding="UTF-8"?>
<config>
  <httpbasic>
    <allow>false</allow>
  </httpbasic>
  <mode>new</mode>

  <origin>
    <login>
      <location>https://websignon.warwick.ac.uk/origin/hs</location>
    </login>
    <logout>
      <location>https://websignon.warwick.ac.uk/origin/logout</location>
    </logout>
    <attributeauthority>
      <location>https://websignon.warwick.ac.uk/origin/aa</location>
    </attributeauthority>
  </origin>

  <shire>
    <filteruserkey>SSO_USER</filteruserkey>
    <uri-header>x-requested-uri</uri-header>
    <location>https://swordfish.warwick.ac.uk/sso/acs</location>
    <sscookie>
      <name>SSO-SSC-Tabula</name>
      <path>/</path>
      <domain>swordfish.warwick.ac.uk</domain>
      <secure>true</secure>
    </sscookie>
    <providerid>urn:swordfish.warwick.ac.uk:start:service</providerid>
  </shire>


  <logout>
    <location>https://swordfish.warwick.ac.uk/sso/logout</location>
  </logout>


  <oauth>
    <enabled>false</enabled>
    <!-- Location of the OAuth service -->
    <service>
      <location>https://websignon.warwick.ac.uk/origin/oauth/service</location>
    </service>
  </oauth>

  <credentials>
    <certificate>file:/Users/alec/warwick/ssl/swordfish.warwick.ac.uk.crt</certificate>
    <key>file:/Users/alec/warwick/ssl/swordfish.warwick.ac.uk.key</key>
    <chain>file:/Users/alec/warwick/ssl/intermediates/quovadis256.pem</chain>
  </credentials>


  <trustedapps>
    <publickey>
      
    </publickey>
    <privatekey>
      
    </privatekey>
    <app>
      <providerid>urn:wellbeing-test.warwick.ac.uk:case-management:service</providerid>
      <publickey>
        
      </publickey>
    </app>
  </trustedapps>

  <masquerade>
    <group>in-elab</group>
  </masquerade>
</config>
